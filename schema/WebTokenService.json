            "type": {
                "href": "/mgmt/types/default/dmReference", 
                "reference-to": {
                    "href": "/mgmt/metadata/default/XMLManager"
                }
            }, 
            "required": "true", 
            "summary": "Assign the XML manager", 
            "cli-alias": "xml-manager", 
            "display": "XML manager", 
            "/mgmt/types/default/dmReference": {
                "type": {
                    "cli-arg": "object", 
