            "/mgmt/types/default/dmXmlSchemaType": {
                "type": {
                    "summary": "The type of XML schema to specify", 
                    "display": "XML Schema Type", 
                    "value-list": {
                        "value": [
                            {
                                "display": "XML", 
                                "name": "xml", 
                                "description": "Specify the XML schema for validation."
