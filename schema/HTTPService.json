            "type": {
                "href": "/mgmt/types/default/dmString"
            }, 
            "display": "Comments"
        }, 
        {
            "name": "Priority", 
            "default": "normal", 
            "type": {
                "href": "/mgmt/types/default/dmSchedulerPriority"
            }, 
