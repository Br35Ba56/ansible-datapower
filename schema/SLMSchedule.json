{
    "schema": [
        {
            "name": "mAdminState", 
            "default": "enabled", 
            "type": {
                "href": "/mgmt/types/default/dmAdminState"
            }, 
            "summary": "Set the administrative state of this configuration.", 
            "/mgmt/types/default/dmAdminState": {
                "type": {
                    "cli-arg": "enabled | disabled", 
                    "name": "dmAdminState", 
                    "value-list": {
                        "value": [
                            {
                                "name": "enabled", 
                                "display": "enabled"
                            }, 
                            {
                                "name": "disabled", 
                                "display": "disabled"
                            }
                        ]
                    }
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmAdminState"
                    }, 
                    "self": {
                        "href": "/mgmt/types/default/dmAdminState"
                    }
                }
            }, 
            "label": "Enable administrative state", 
            "cli-alias": "admin-state", 
            "display": "Administrative state", 
            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>", 
            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
        }, 
        {
            "/mgmt/types/default/dmString": {
                "type": {
                    "cli-arg": "string", 
                    "name": "dmString"
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmString"
                    }, 
                    "self": {
                        "href": "/mgmt/types/default/dmString"
                    }
                }
            }, 
            "name": "UserSummary", 
            "cli-alias": "summary", 
            "summary": "Brief summary for user annotation.", 
            "type": {
                "href": "/mgmt/types/default/dmString"
            }, 
            "display": "Comments"
        }, 
        {
            "name": "DaysOfWeek", 
            "type": {
                "href": "/mgmt/types/default/dmWeekdayBitmap"
            }, 
            "required": "true", 
            "summary": "Days of the week to enforce", 
            "/mgmt/types/default/dmWeekdayBitmap": {
                "type": {
                    "summary": "Days of Week", 
                    "display": "Days of Week", 
                    "value-list": {
                        "value": [
                            {
                                "name": "Sunday", 
                                "display": "Sunday"
                            }, 
                            {
                                "name": "Monday", 
                                "display": "Monday"
                            }, 
                            {
                                "name": "Tuesday", 
                                "display": "Tuesday"
                            }, 
                            {
                                "name": "Wednesday", 
                                "display": "Wednesday"
                            }, 
                            {
                                "name": "Thursday", 
                                "display": "Thursday"
                            }, 
                            {
                                "name": "Friday", 
                                "display": "Friday"
                            }, 
                            {
                                "name": "Saturday", 
                                "display": "Saturday"
                            }
                        ]
                    }, 
                    "name": "dmWeekdayBitmap"
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmWeekdayBitmap"
                    }, 
                    "self": {
                        "href": "/mgmt/types/default/dmWeekdayBitmap"
                    }
                }
            }, 
            "cli-alias": "days", 
            "display": "Week Days", 
            "description": "The days of the week to enforce the schedule. The time and duration apply to all selected days. Enter &lt;b>Saturday&lt;/b> and &lt;b>Sunday&lt;/b> to define a weekend schedule."
        }, 
        {
            "name": "StartTime", 
            "display": "Start Time", 
            "type": {
                "href": "/mgmt/types/default/dmTimeOfDay"
            }, 
            "required": "true", 
            "summary": "Start time to enforce", 
            "valid-regexp": "^(([01][0-9])|(2[0-3])):[0-5][0-9]:[0-5][0-9]$", 
            "units": "hh:mm:ss", 
            "/mgmt/types/default/dmTimeOfDay": {
                "type": {
                    "cli-arg": "time HH:MM", 
                    "summary": "Time of day in minute resolution using 24 hour time semantics", 
                    "display": "HH:MM", 
                    "name": "dmTimeOfDay", 
                    "description": "Time of day in minute resolution using 24 hour time semantics."
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmTimeOfDay"
                    }, 
                    "self": {
                        "href": "/mgmt/types/default/dmTimeOfDay"
                    }
                }
            }, 
            "cli-alias": "start", 
            "example": "12:34:00", 
            "description": "The start time to enforce the schedule in the current timezone in the 24-hour format. To enforce from 8:00 AM to 8:30 PM, enter 08:00:00."
        }, 
        {
            "/mgmt/types/default/dmUInt32": {
                "type": {
                    "cli-arg": "number", 
                    "minimum": 0, 
                    "name": "dmUInt32", 
                    "maximum": "0xFFFFFFFF"
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmUInt32"
                    }, 
                    "self": {
                        "href": "/mgmt/types/default/dmUInt32"
                    }
                }
            }, 
            "name": "Duration", 
            "default": 1440, 
            "type": {
                "href": "/mgmt/types/default/dmUInt32"
            }, 
            "required": "true", 
            "maximum": 1440, 
            "summary": "Number of minutes after the start time to enforce", 
            "minimum": 1, 
            "units": "Minutes", 
            "cli-alias": "duration", 
            "display": "Duration", 
            "description": "The number of minutes to enforce the schedule. Enter any value of 1 - 1440. The default is 1440. To enforce from 8:00 AM to 8:30 PM, enter 750."
        }, 
        {
            "name": "StartDate", 
            "valid-regexp": "^20[0-9][0-9]-((0[1-9])|(1[0-2]))-((0[1-9])|([12][0-9])|(3[01]))$", 
            "summary": "Start date to enforce", 
            "example": "2011-08-24", 
            "type": {
                "href": "/mgmt/types/default/dmDate"
            }, 
            "/mgmt/types/default/dmDate": {
                "type": {
                    "cli-arg": "date yyyy-MM-dd", 
                    "name": "dmDate"
                }, 
                "_links": {
                    "doc": {
