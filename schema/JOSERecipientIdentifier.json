                "type": {
                    "name": "dmDecryptKeyType", 
                    "value-list": {
                        "value": [
                            {
                                "summary": "Private Key", 
                                "display": "Private Key", 
                                "name": "key"
                            }, 
                            {
                                "summary": "Shared Secret Key", 
                                "display": "Shared Secret Key", 
                                "name": "sskey"
                            }
