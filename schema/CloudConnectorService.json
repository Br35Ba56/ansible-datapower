                "type": {
                    "cli-arg": "dotted-ip or hostname", 
                    "name": "dmHostname", 
                    "format": "(a.b.c.d or hostname)"
                }, 
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmHostname"
                    }, 
